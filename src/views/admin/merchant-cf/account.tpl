<h1>Merchant_CF_Account</h1>
<form id="Login" action="/wp-admin/admin.php?page=woo-fpt-ups&controller=merchantCF_Country">
    <legend><span>Contry Setting</span></legend>
    <table class="form-table">
        <tr valign="top">
            <th scope="row" class="titledesc">
                <label for="woocommerce_flexible_shipping_ups_account_number">Country <span class="woocommerce-help-tip"></span></label>
            </th>
            <td class="forminp">
                <fieldset>
                    <legend class="screen-reader-text"><span>UPS Account Number</span></legend>
                    <input class="input-text regular-input " type="text" name="woocommerce_flexible_shipping_ups_account_number" id="woocommerce_flexible_shipping_ups_account_number" style="" value="" placeholder="" required="required">
                </fieldset>
            </td>
        </tr>
        <tr valign="top">
            <th scope="row" class="titledesc">
                <label for="woocommerce_flexible_shipping_ups_password">UPS Password <span class="woocommerce-help-tip"></span></label>
            </th>
            <td class="forminp">
                <fieldset>
                    <legend class="screen-reader-text"><span>UPS Password</span></legend>
                    <input class="input-text regular-input " type="password" name="woocommerce_flexible_shipping_ups_password" id="woocommerce_flexible_shipping_ups_password" style="" value="" placeholder="" required="required" autocomplete="new-password">
                </fieldset>
            </td>
        </tr>
        <tr valign="top">
            <td class="forminp">
                <button class="button button-primary">Continue</button>
            </td>
        </tr>
    </table>
</form>